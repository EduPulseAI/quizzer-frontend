{
  "name": "coding-quizzer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/coding-quizzer/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/coding-quizzer"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/coding-quizzer"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "coding-quizzer:build",
        "dev": true,
        "turbo": true
      },
      "configurations": {
        "development": {
          "buildTarget": "coding-quizzer:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "coding-quizzer:build:production",
          "dev": false
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/coding-quizzer/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
